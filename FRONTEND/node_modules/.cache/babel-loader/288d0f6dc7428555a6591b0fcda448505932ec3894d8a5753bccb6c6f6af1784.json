l-5\" type=\"text\" placeholder=\"Entrez votre adresse email\" required/>\n        <button id=\"submitButton\" class=\"submit-button flex items-center font-thin opacity-100 text-white hover:font-bold bg-black h-full pr-5 pl-5\">\n          <h1 type=\"submit\">S'inscrire</h1>\n        </button>\n      </div>\n      <div class=\"flex items-center mt-1\">\n         <input id=\"privacyCheckbox\" type=\"checkbox\" name=\"politiqueConfidentialité\" required>\n         <label class=\"ml-2 font-thin\" for=\"privacyCheckbox\">J'accepte la <a href=\"#\" class=\"underline font-bold hover:text-white\" style=\"transition:all .5s ease-in-out\">politique de confidentialité</a></label>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n    // Retrieve the necessary elements from the DOM\n    const emailInput = document.getElementById('emailInput');\n    const submitButton = document.getElementById('submitButton');\n    const privacyCheckbox = document.getElementById('privacyCheckbox');\n  \n    // Function to display an error message\n    const displayErrorMessage = (element, message) => {\n      const errorBubble = document.createElement('div');\n      errorBubble.className = 'error-bubble';\n      errorBubble.textContent = message;\n      element.parentNode.insertBefore(errorBubble, element.nextSibling);\n    };\n  \n    // Remove any existing error messages\n    const removeErrorMessages = () => {\n      const errorBubbles = document.getElementsByClassName('error-bubble');\n      while (errorBubbles.length > 0) {\n        errorBubbles[0].parentNode.removeChild(errorBubbles[0]);\n      }\n    };\n  \n    // Validate the form on submit\n    const validateForm = (event) => {\n      event.preventDefault(); // Prevent form submission\n  \n      removeErrorMessages(); // Remove any existing error messages\n  \n      if (emailInput.value.trim() === '') {\n        displayErrorMessage(emailInput, 'Veuillez entrer votre adresse email');\n      }\n  \n      if (!privacyCheckbox.checked) {\n        displayErrorMessage(privacyCheckbox, 'Vous devez accepter la politique de confidentialité');\n      }\n    };\n  \n    // Add event listener to the submit button\n    submitButton.addEventListener('click', validateForm);\n  </script>\n  \n  <style>\n  .newsletter-bg {\n    background-color: burlywood;\n    height: 25vh;\n    width: 100%;\n  }\n  \n  .error-bubble {\n    color: red;\n    font-size: 14px;\n    margin-top: 5px;\n  }\n  </style>\n  "],"mappings":";;EACSA,KAAK,EAAC;AAA0C;;oBACnDC,UAAmD,C;;uBADrDC,mBAAA,CAYM,OAZNC,UAYM,EAAAC,UAAA"},"metadata":{},"sourceType":"module","externalDependencies":[]}