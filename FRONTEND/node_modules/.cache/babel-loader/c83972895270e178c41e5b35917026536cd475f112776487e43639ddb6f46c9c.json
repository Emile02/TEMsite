iv class=\"mb-4\">\n          <label class=\"block text-gray-700 font-bold mb-2\" for=\"personnes\">\n            Nombre de personnes :\n          </label>\n          <input\n            v-model=\"personnes\"\n            class=\"shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline\"\n            id=\"personnes\"\n            type=\"number\"\n            placeholder=\"Entrez le nombre de personnes\"\n            required\n          />\n        </div>\n        <div class=\"flex justify-center\">\n          <button\n            type=\"submit\"\n            class=\"bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded focus:outline-none focus:shadow-outline\"\n          >\n            Réserver\n          </button>\n        </div>\n      </form>\n    </div>\n  </template>\n  \n  <script>\n  import axios from 'axios';\n  \n  export default {\n    // ...\n  \n    methods: {\n      submitReservation() {\n        const reservationData = {\n          nom: this.nom,\n          heure: this.heure,\n          personnes: this.personnes\n        };\n  \n        axios.post('/api/reservations', reservationData)\n          .then((response) => {\n            console.log(response.data.message);\n          })\n          .catch((error) => {\n            console.error('Erreur lors de la soumission de la réservation:', error);\n          });\n      }\n    }\n  };\n  </script>"],"mappings":"AAuDE,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACb;;EAEAC,OAAO,EAAE;IACPC,iBAAiBA,CAAA,EAAG;MAClB,MAAMC,eAAc,GAAI;QACtBC,GAAG,EAAE,IAAI,CAACA,GAAG;QACbC,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,SAAS,EAAE,IAAI,CAACA;MAClB,CAAC;MAEDN,KAAK,CAACO,IAAI,CAAC,mBAAmB,EAAEJ,eAAe,EAC5CK,IAAI,CAAEC,QAAQ,IAAK;QAClBC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACG,IAAI,CAACC,OAAO,CAAC;MACpC,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;QAChBL,OAAO,CAACK,KAAK,CAAC,iDAAiD,EAAEA,KAAK,CAAC;MACzE,CAAC,CAAC;IACN;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependenci